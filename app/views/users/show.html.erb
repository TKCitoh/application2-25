<h1>User info</h1>
<%= form_with model: @book do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>name</h4>
  <%= f.text_field :name %>

  <h4>introduction</h4>
  <%= f.text_field :nemw %>


  <%= f.submit '投稿' %>
<% end %>

<h1>New Book</h1>
  <%= form_with model: @book do |f| %>
    <h4>Title</h4>
      <%= f.text_field :title %>
    <h4>Opinion</h4>
      <%= f.text_area :body%>
    <%= f.submit 'Create Book' %>
  <% end %>

<h1>Books</h1>
<h4>Title</h4>
<h4>Opinion</h4>

